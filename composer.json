{
    "name": "nswdpc/silverstripe-pruner",
    "description": "Prune records from configured DataObject classes",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "pruning",
        "delete",
        "remove",
        "truncate"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James Ellis",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "NSWDPC\\Prune\\": "src/",
            "NSWDPC\\Prune\\Tests\\": "tests/"
        }
    },
    "require":
    {
        "php": "^7.2|^8",
        "silverstripe/framework": "^4.0",
        "symbiote/silverstripe-queuedjobs": "^4.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^3"
    }
}
